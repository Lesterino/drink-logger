<%- include('../partials/header') %>
<h3>All Places</h3>
<div>
    <% places.forEach((p) => { %> 
    <ul>
        <li> 
            <%= `${p.name} -- ${p.location}` %> <a href="/places/<%= p._id %>">Details</a>
        </li>
    </ul>
    <% }); %>
</div>
<a href="../places/new">Add new place</a>
